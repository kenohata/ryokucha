<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>

<% if logged_in? %>
  You're logged in.
  <%= link_to 'logout', sessions_path, method: :delete %>
<% else %>
  You're not logged in.
<% end %>

<%= form_for @user, url: sessions_path, method: :post do |f| %>
  <%= f.text_field :email %>
  <%= f.password_field :password %>
  <%= f.submit %>
<% end %>
